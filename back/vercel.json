{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node",
            "config": {
              "command": "npm run build && npm start"
            }
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "app.js"
        }
    ]
}